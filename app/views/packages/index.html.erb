<h1>CRAN index</h1>

<table class="table">
  <thead>
  <th>
    Name
  </th>
  <th>
    Version
  </th>
  <th>
    Date/Publication
  </th>
  <th>
    Title
  </th>
  <th>
    Description
  </th>
  <th>
    Author
  </th>
  <th>
    Maintainer
  </th>
  </thead>
  <tbody>
  <% @packages.each do |package| %>
    <tr class="package">
      <td>
        <%= package.name %>
      </td>
      <td>
        <%= package.version %>
      </td>
      <td>
        <%= package.date_publication %>
      </td>
      <td>
        <%= package.title %>
      </td>
      <td>
        <%= package.description %>
      </td>
      <td>
        <%= package.authors %>
      </td>
      <td>
        <%= package.maintainers %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>
